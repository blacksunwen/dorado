<?xml version="1.0" encoding="UTF-8"?>
<config>
	<Patterns>
		<Pattern name="dorado-js" contentType="text/javascript" charset="UTF-8" mergeRequests='${configure["view.mergeJavaScript"]}' baseUri=">dorado/client"
			resourceSuffix=".js" />
		<Pattern name="dorado-css" contentType="text/css" charset="UTF-8" mergeRequests='${configure["view.mergeStyleSheet"]}' baseUri=">dorado/client/skins/~current"
			resourceSuffix=".css" />
		<Pattern name="res-js" contentType="text/javascript" charset="UTF-8" mergeRequests='${configure["view.mergeJavaScript"]}' resourceSuffix=".js" />
		<Pattern name="res-css" contentType="text/css" charset="UTF-8" mergeRequests="false" resourceSuffix=".css" />
	</Patterns>

	<Packages>
		<Package name="jquery" pattern="dorado-js">
			scripts/jquery/jquery-1.8.3
		</Package>

		<Package name="jquery-plugins" pattern="dorado-js" depends="jquery">
			scripts/jquery/jquery.easing.1.3,
			scripts/jquery/jquery.mousewheel,
			scripts/jquery/jquery.hotkeys,
			scripts/jquery/jquery-ui-1.9.3.custom,
			scripts/jquery/jquery.swfobject.1-1-1,
			scripts/jquery/jquery.textarea
		</Package>

		<Package name="json2" pattern="dorado-js">
			scripts/json/json2
		</Package>

		<Package name="core" pattern="dorado-js" depends="jquery,json2">
			scripts/dorado/core,
			resources/i18n/core
		</Package>

		<Package name="data" pattern="dorado-js" depends="core">
			scripts/dorado/data,
			resources/i18n/data
		</Package>

		<Package name="widget-skin" pattern="dorado-css">
			skins/~current/common,
			skins/~current/widget
		</Package>
		<Package name="widget" pattern="dorado-js" depends="jquery-plugins,data,widget-skin">
			scripts/dorado/widget,
			resources/i18n/widget,
			skins/~current/support
		</Package>

		<Package name="base-widget-skin" pattern="dorado-css">
			skins/~current/base-widget
		</Package>
		<Package name="base-widget" pattern="dorado-js" depends="widget,base-widget-skin">
			scripts/dorado/base-widget,
			resources/i18n/base-widget
		</Package>

		<Package name="list-skin" pattern="dorado-css">
			skins/~current/list
		</Package>
		<Package name="list" pattern="dorado-js" depends="base-widget,list-skin">
			scripts/dorado/list,
			resources/i18n/list
		</Package>

		<Package name="grid-skin" pattern="dorado-css" depends="list-skin">
			skins/~current/grid
		</Package>
		<Package name="grid" pattern="dorado-js" depends="list,grid-skin">
			scripts/dorado/grid,
			resources/i18n/grid
		</Package>

		<Package name="tree-skin" pattern="dorado-css">
			skins/~current/tree
		</Package>
		<Package name="tree" pattern="dorado-js" depends="list,tree-skin">
			scripts/dorado/tree,
			resources/i18n/tree
		</Package>

		<Package name="block-view-skin" pattern="dorado-css">
			skins/~current/block-view
		</Package>
		<Package name="block-view" pattern="dorado-js" depends="list,block-view-skin">
			scripts/dorado/block-view
		</Package>

		<Package name="tree-grid" pattern="dorado-js" depends="grid,tree">
			scripts/dorado/tree-grid
		</Package>

		<Package name="debugger-skin" pattern="dorado-css">
			skins/~current/debugger
		</Package>
		<Package name="debugger" pattern="dorado-js" depends="base-widget,debugger-skin">
			scripts/dorado/debugger
		</Package>
	</Packages>
</config>
