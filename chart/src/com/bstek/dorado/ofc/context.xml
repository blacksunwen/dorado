<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:d="http://www.bstek.com/dorado/schema"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://www.bstek.com/dorado/schema http://www.bstek.com/dorado/schema/spring-dorado-7.0.xsd">
	<bean parent="dorado.packagesConfigLoader">
		<property name="configLocation" value="com/bstek/dorado/ofc/packages-config.xml" />
	</bean>

	<bean id="dorado.OpenFlashChart.ElementsParser" parent="dorado.collectionToPropertyParser">
		<property name="property" value="elements" />
		<d:impl-type name="com.bstek.dorado.ofc.element.Pie" />
		<d:impl-type name="com.bstek.dorado.ofc.element.Column" />
		<d:impl-type name="com.bstek.dorado.ofc.element.FilledColumn" />
		<d:impl-type name="com.bstek.dorado.ofc.element.SketchColumn" />
		<d:impl-type name="com.bstek.dorado.ofc.element.StackedColumn" />
		<d:impl-type name="com.bstek.dorado.ofc.element.Bar" />
		<d:impl-type name="com.bstek.dorado.ofc.element.Line" />
		<d:impl-type name="com.bstek.dorado.ofc.element.Area" />
		<d:impl-type name="com.bstek.dorado.ofc.element.Scatter" />
		<d:impl-type name="com.bstek.dorado.ofc.element.Shape" />
		<d:impl-type name="com.bstek.dorado.ofc.element.Tags" />
	</bean>

	<bean id="com.bstek.dorado.ofc.OpenFlashChart" parent="dorado.defaultComponentTypeRegister" />
</beans>
