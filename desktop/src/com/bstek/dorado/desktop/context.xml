<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:d="http://www.bstek.com/dorado/schema"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://www.bstek.com/dorado/schema http://www.bstek.com/dorado/schema/spring-dorado-7.0.xsd">
	<bean parent="dorado.packagesConfigLoader">
		<property name="configLocation" value="com/bstek/dorado/desktop/packages-config.xml" />
	</bean>

	<bean parent="dorado.idesupport.ruleConfigLoader">
		<property name="configLocation" value="com/bstek/dorado/desktop/config-rules.xml" />
	</bean>

	<bean id="dorado.Desktop.AppsParser" parent="dorado.collectionToPropertyParser">
		<property name="property" value="apps" />
		<d:impl-type name="com.bstek.dorado.desktop.DialogApp" />
		<d:impl-type name="com.bstek.dorado.desktop.WidgetApp" />
		<d:impl-type name="com.bstek.dorado.desktop.IFrameApp" />
	</bean>

	<bean id="com.bstek.dorado.desktop.Flash" parent="dorado.defaultComponentTypeRegister" />
	<bean id="com.bstek.dorado.desktop.Desktop" parent="dorado.defaultComponentTypeRegister" />
	<bean id="com.bstek.dorado.desktop.DesktopCarousel" parent="dorado.defaultComponentTypeRegister" />
	<bean id="com.bstek.dorado.desktop.Taskbar" parent="dorado.defaultComponentTypeRegister" />
	<bean id="com.bstek.dorado.desktop.Shell" parent="dorado.defaultComponentTypeRegister" />
</beans>
