<?xml version="1.0" encoding="UTF-8"?>
<DbModel jdbcEnviroment="mysql">
	<Table name="EMPLOYEE" tableName="EMPLOYEE">
	  <Columns>
	    <KeyColumn jdbcType="INTEGER-Integer" columnName="ID"/>
	    <Column jdbcType="INTEGER-Integer" columnName="REPORT_TO"/>
	    
	    <Column jdbcType="DATE-Date" columnName="HIRE_DATE"/>
	    <Column jdbcType="DATE-Date" columnName="BIRTHDAY"/>
	    
	    <Column jdbcType="BIT-Boolean" columnName="SEX"/>
	    
	    <Column jdbcType="LONGVARCHAR-String" columnName="NOTES"/>
	    
	    <Column jdbcType="VARCHAR-String" columnName="DEPT_ID"/>
	    <Column jdbcType="VARCHAR-String" columnName="COUNTRY"/>
	    <Column jdbcType="VARCHAR-String" columnName="ADDRESS"/>
	    <Column jdbcType="VARCHAR-String" columnName="LAST_NAME"/>
	    <Column jdbcType="VARCHAR-String" columnName="PHONE"/>
	    <Column jdbcType="VARCHAR-String" columnName="FIRST_NAME"/>
	    <Column jdbcType="VARCHAR-String" columnName="POSTAL_CODE"/>
	    <Column jdbcType="VARCHAR-String" columnName="CITY"/>
	    <Column jdbcType="VARCHAR-String" columnName="PHOTO_PATH"/>
	    <Column jdbcType="VARCHAR-String" columnName="REGION"/>
	    <Column jdbcType="VARCHAR-String" columnName="TITLE_OF_COURTESY"/>
	    <Column jdbcType="VARCHAR-String" columnName="TITLE"/>
	  </Columns>
	  <Property name="dynamicClause"><![CDATA[#if ($ID) WHERE ID=:ID #end]]></Property>
	</Table>
	
	<Table name="DEPT" tableName="DEPT">
	  <Columns>
	    <KeyColumn jdbcType="VARCHAR-String" columnName="DEPT_ID"/>
	    <Column jdbcType="VARCHAR-String" columnName="DEPT_NAME"/>
	    <Column jdbcType="VARCHAR-String" columnName="PARENT_ID"/>
	  </Columns>
	</Table>
</DbModel>