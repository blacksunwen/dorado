<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean parent="dorado.idesupport.ruleConfigLoader">
		<property name="configLocation" value="com/bstek/dorado/jdbc/jdbc-config-rules.xml"/>
		<property name="initializerMap">
			<map>
				<entry key="AbstractDbColumn">
					<bean class="com.bstek.dorado.jdbc.ide.AbstractDbColumnRuleTemplateInitializer"/>
				</entry>
				<entry key="TableKeyColumn">
					<bean class="com.bstek.dorado.jdbc.ide.TableKeyColumnRuleTemplateInitializer"/>
				</entry>
				<entry key="JdbcDataResolver">
					<bean class="com.bstek.dorado.jdbc.ide.JdbcDataResolverRuleTemplateInitializer"/>
				</entry>
				<entry key="Table">
					<bean class="com.bstek.dorado.jdbc.ide.TableRuleTemplateInitializer"/>
				</entry>
			</map>
		</property>
	</bean>
</beans>
