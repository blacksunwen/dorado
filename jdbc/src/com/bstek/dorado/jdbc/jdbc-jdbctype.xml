<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="dorado.jdbc.abstractJdbcType" abstract="true">
		<property name="manager" ref="dorado.jdbc.jdbcTypeManager"/>
	</bean>

	<bean class="com.bstek.dorado.jdbc.type.BitBoolean" parent="dorado.jdbc.abstractJdbcType"/>
	<bean class="com.bstek.dorado.jdbc.type.BooleanBoolean" parent="dorado.jdbc.abstractJdbcType"/>
	<bean class="com.bstek.dorado.jdbc.type.CharBoolean" parent="dorado.jdbc.abstractJdbcType"/>
	<bean class="com.bstek.dorado.jdbc.type.SmallintShort" parent="dorado.jdbc.abstractJdbcType"/>
	<bean class="com.bstek.dorado.jdbc.type.IntegerInteger" parent="dorado.jdbc.abstractJdbcType"/>
	<bean class="com.bstek.dorado.jdbc.type.BigintLong" parent="dorado.jdbc.abstractJdbcType"/>
	<bean class="com.bstek.dorado.jdbc.type.RealFloat" parent="dorado.jdbc.abstractJdbcType"/>
	<bean class="com.bstek.dorado.jdbc.type.FloatDouble" parent="dorado.jdbc.abstractJdbcType"/>
	<bean class="com.bstek.dorado.jdbc.type.DoubleDouble" parent="dorado.jdbc.abstractJdbcType"/>
	<bean class="com.bstek.dorado.jdbc.type.NumricBigDecimal" parent="dorado.jdbc.abstractJdbcType"/>
	<bean class="com.bstek.dorado.jdbc.type.DecimalBigDecimal" parent="dorado.jdbc.abstractJdbcType"/>
	<bean class="com.bstek.dorado.jdbc.type.TinyintByte" parent="dorado.jdbc.abstractJdbcType"/>
	<bean class="com.bstek.dorado.jdbc.type.DateDate" parent="dorado.jdbc.abstractJdbcType"/>
	<bean class="com.bstek.dorado.jdbc.type.TimeTime" parent="dorado.jdbc.abstractJdbcType"/>
	<bean class="com.bstek.dorado.jdbc.type.TimestampDateTime" parent="dorado.jdbc.abstractJdbcType"/>
    <bean class="com.bstek.dorado.jdbc.type.VarcharString" parent="dorado.jdbc.abstractJdbcType"/>
    <bean class="com.bstek.dorado.jdbc.type.LongvarcharString" parent="dorado.jdbc.abstractJdbcType"/>
    <bean class="com.bstek.dorado.jdbc.type.ClobString" parent="dorado.jdbc.abstractJdbcType"/>
</beans>
