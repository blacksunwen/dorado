<?xml version="1.0" encoding="UTF-8"?>
<ViewConfig>
  <Arguments/>
  <Context/>
  <Model>
    <DataType name="commonData">
      <PropertyDef name="value"/>
    </DataType>
  </Model>
  <View>
    <DataSet id="dsLine">
      <Property name="dataType">[commonData]</Property>
      <Property name="dataProvider">populationDataProvider#getLineData</Property>
    </DataSet>
    <DataSet id="dsArea">
      <Property name="dataType">[commonData]</Property>
      <Property name="dataProvider">populationDataProvider#getAreaData</Property>
    </DataSet>
    <DataSet id="dsBar">
      <Property name="dataType">[commonData]</Property>
      <Property name="dataProvider">populationDataProvider#getBarData</Property>
    </DataSet>
    <OpenFlashChart id="openFlashChart1">
      <Property name="backgroundColor">#ffffff</Property>
      <Property name="title">
        <Property name="text">PopulationChart</Property>
      </Property>
      <Elements>
        <Column>
          <Property name="dataSet">dsBar</Property>
          <Property name="bindingConfig">
            <Property name="valueProperty">value</Property>
          </Property>
          <Property name="text">Population</Property>
          <Property name="color">#afd8f8</Property>
          <Property name="type">Glass</Property>
        </Column>
        <Line>
          <Property name="bindingConfig">
            <Property name="valueProperty">value</Property>
          </Property>
          <Property name="dataSet">dsLine</Property>
          <Property name="text">Birth Rate</Property>
          <Property name="axis">right</Property>
          <Property name="color">#f6bd0f</Property>
        </Line>
        <Area>
          <Property name="dataSet">dsArea</Property>
          <Property name="bindingConfig">
            <Property name="valueProperty">value</Property>
          </Property>
          <Property name="text">Death Rate</Property>
          <Property name="axis">right</Property>
          <Property name="color">#c2da79</Property>
        </Area>
      </Elements>
      <XAxis>
        <Property name="labelsShortcut">2000,2001,2002,2003,2004,2005</Property>
      </XAxis>
      <YAxis>
        <Property name="max">300</Property>
        <Property name="min">260</Property>
        <Property name="steps">10</Property>
        <Labels>
          <Property name="text">#val#M</Property>
        </Labels>
      </YAxis>
      <YAxisRight>
        <Property name="max">2</Property>
        <Property name="min">0</Property>
        <Property name="steps">0.5</Property>
      </YAxisRight>
    </OpenFlashChart>
  </View>
</ViewConfig>
