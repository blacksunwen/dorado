<?xml version="1.0" encoding="UTF-8"?>
<ViewConfig>
  <Arguments/>
  <Context/>
  <Model/>
  <View>
    <Shell>
      <Property name="wallpaper">>dorado/res/com/bstek/dorado/sample/desktop/wallpapers/dorado1.jpg</Property>
      <Desktop>
        <DesktopCarousel>
          <Desktop>
            <Property name="iconSize">small</Property>
            <Shortcut>
              <Property name="appId">iframe1</Property>
            </Shortcut>
            <Shortcut>
              <Property name="appId">iframe2</Property>
            </Shortcut>
            <Shortcut>
              <Property name="appId">iframe3</Property>
            </Shortcut>
            <Shortcut>
              <Property name="appId">iframe4</Property>
            </Shortcut>
          </Desktop>
          <Desktop>
            <Shortcut>
              <Property name="appId">chart1</Property>
            </Shortcut>
            <Shortcut>
              <Property name="appId">chart2</Property>
            </Shortcut>
            <Shortcut>
              <Property name="appId">chart3</Property>
            </Shortcut>
            <Shortcut>
              <Property name="appId">chart4</Property>
            </Shortcut>
            <Shortcut>
              <Property name="appId">chart5</Property>
            </Shortcut>
            <Shortcut>
              <Property name="appId">chart6</Property>
            </Shortcut>
          </Desktop>
          <Desktop>
            <Shortcut>
              <Property name="appId">widget1</Property>
            </Shortcut>
          </Desktop>
        </DesktopCarousel>
      </Desktop>
      <Apps>
        <WidgetApp>
          <Property name="id">widget1</Property>
          <Property name="icon32">>dorado/res/com/bstek/dorado/sample/desktop/clock.png</Property>
          <Property name="caption">Clock Widget</Property>
          <Flash>
            <Property name="path">>dorado/res/com/bstek/dorado/sample/desktop/black_clock.swf</Property>
            <Property name="width">150</Property>
            <Property name="height">150</Property>
          </Flash>
        </WidgetApp>
        <IFrameApp>
          <Property name="id">iframe1</Property>
          <Property name="caption">Grid</Property>
          <Property name="path">>com.bstek.dorado.sample.data.SimpleCRUD.d</Property>
          <Property name="icon32">>dorado/res/com/bstek/dorado/sample/desktop/grid.png</Property>
        </IFrameApp>
        <IFrameApp>
          <Property name="id">iframe2</Property>
          <Property name="caption">Form</Property>
          <Property name="path">>com.bstek.dorado.sample.data.Validators.d</Property>
          <Property name="icon32">>dorado/res/com/bstek/dorado/sample/desktop/form.png</Property>
          <Property name="width">700</Property>
          <Property name="height">420</Property>
        </IFrameApp>
        <IFrameApp>
          <Property name="id">iframe3</Property>
          <Property name="caption">Tree</Property>
          <Property name="path">>com.bstek.dorado.sample.data.RecursiveTree.d</Property>
          <Property name="icon32">>dorado/res/com/bstek/dorado/sample/desktop/tree.png</Property>
          <Property name="width">360</Property>
          <Property name="height">500</Property>
        </IFrameApp>
        <IFrameApp>
          <Property name="id">iframe4</Property>
          <Property name="caption">Tree Grid</Property>
          <Property name="path">>com.bstek.dorado.sample.component.TreeGrid.d</Property>
          <Property name="icon32">>dorado/res/com/bstek/dorado/sample/desktop/treegrid.png</Property>
        </IFrameApp>
        <IFrameApp>
          <Property name="id">iframe5</Property>
          <Property name="caption">Google</Property>
          <Property name="path">http://www.baidu.com</Property>
          <Property name="icon32">>dorado/res/com/bstek/dorado/sample/desktop/phone.png</Property>
        </IFrameApp>
        <IFrameApp>
          <Property name="id">chart1</Property>
          <Property name="caption">Area</Property>
          <Property name="path">>com.bstek.dorado.sample.ofc.Area.d</Property>
          <Property name="icon32">>dorado/res/com/bstek/dorado/sample/desktop/area.png</Property>
          <Property name="icon16">>dorado/res/com/bstek/dorado/sample/desktop/area-small.png</Property>
        </IFrameApp>
        <IFrameApp>
          <Property name="id">chart2</Property>
          <Property name="caption">Bar</Property>
          <Property name="path">>com.bstek.dorado.sample.ofc.Bar.d</Property>
          <Property name="icon32">>dorado/res/com/bstek/dorado/sample/desktop/bar.png</Property>
          <Property name="icon16">>dorado/res/com/bstek/dorado/sample/desktop/bar-small.png</Property>
        </IFrameApp>
        <IFrameApp>
          <Property name="id">chart3</Property>
          <Property name="caption">Column</Property>
          <Property name="path">>com.bstek.dorado.sample.ofc.Column.d</Property>
          <Property name="icon32">>dorado/res/com/bstek/dorado/sample/desktop/column.png</Property>
        </IFrameApp>
        <IFrameApp>
          <Property name="id">chart4</Property>
          <Property name="caption">Line</Property>
          <Property name="path">>com.bstek.dorado.sample.ofc.Line.d</Property>
          <Property name="icon32">>dorado/res/com/bstek/dorado/sample/desktop/line.png</Property>
        </IFrameApp>
        <IFrameApp>
          <Property name="id">chart5</Property>
          <Property name="caption">Pie</Property>
          <Property name="path">>com.bstek.dorado.sample.ofc.Pie.d</Property>
          <Property name="icon32">>dorado/res/com/bstek/dorado/sample/desktop/pie.png</Property>
        </IFrameApp>
        <IFrameApp>
          <Property name="id">chart6</Property>
          <Property name="caption">Pie</Property>
          <Property name="path">>com.bstek.dorado.sample.ofc.Radar.d</Property>
          <Property name="icon32">>dorado/res/com/bstek/dorado/sample/desktop/radar.png</Property>
        </IFrameApp>
        <DialogApp>
          <Property name="id">control1</Property>
          <Property name="icon32">>dorado/res/com/bstek/dorado/sample/desktop/pie.png</Property>
          <Dialog>
            <Property name="height">400</Property>
            <Property name="width">600</Property>
            <Property name="caption">Test</Property>
            <Children/>
          </Dialog>
        </DialogApp>
      </Apps>
      <Taskbar>
        <StartButton>
          <SimpleButton>
            <ClientEvent name="onClick">var startMenuPanel = this.id(&quot;startMenuPanel&quot;);
startMenuPanel.addListener(&quot;onShow&quot;, function() {
	self.set(&quot;toggled&quot;, true);
	startMenuPanel.addListener(&quot;onHide&quot;, function() {
		self.set(&quot;toggled&quot;, false);
	}, {once: true});
}, {once: true});
startMenuPanel.show({
	anchorTarget: self,
	vAlign: &quot;top&quot;,
	align: &quot;innerleft&quot;,
	positionOffset: [0, 8]
});</ClientEvent>
            <Property name="className">start-button</Property>
          </SimpleButton>
        </StartButton>
        <QuickButtons>
          <QuickButton>
            <Property name="appId">chart1</Property>
          </QuickButton>
          <QuickButton>
            <Property name="appId">chart2</Property>
          </QuickButton>
        </QuickButtons>
      </Taskbar>
    </Shell>
    <FloatContainer id="startMenuPanel">
      <ClientEvent name="onBlur">self.hide();
</ClientEvent>
      <Property name="width">330</Property>
      <Property name="height">300</Property>
      <Property name="animateType">fade</Property>
      <Property name="style">
        <Property name="position">absolute</Property>
      </Property>
      <Children>
        <Panel>
          <Property name="exClassName">start-menu-panel</Property>
          <Property name="caption">Desktop</Property>
          <Children>
            <Menu layoutConstraint="left width:150">
              <Property name="exClassName">left-menu</Property>
              <Property name="floating">false</Property>
              <MenuItem>
                <Property name="caption">Settings</Property>
                <Property name="icon">>dorado/res/com/bstek/dorado/sample/desktop/left-default-menu-icon.png</Property>
              </MenuItem>
              <MenuItem>
                <Property name="caption">Logout</Property>
                <Property name="icon">>dorado/res/com/bstek/dorado/sample/desktop/left-default-menu-icon.png</Property>
              </MenuItem>
            </Menu>
            <Menu>
              <Property name="exClassName">right-menu</Property>
              <Property name="floating">false</Property>
              <MenuItem>
                <Property name="caption">Demo</Property>
                <MenuItem>
                  <Property name="caption">Grid</Property>
                </MenuItem>
                <MenuItem>
                  <Property name="caption">Form</Property>
                </MenuItem>
              </MenuItem>
              <MenuItem>
                <Property name="caption">Chart</Property>
                <MenuItem>
                  <Property name="caption">Column Chart</Property>
                </MenuItem>
                <MenuItem>
                  <Property name="caption">Bar Chart</Property>
                </MenuItem>
                <MenuItem>
                  <Property name="caption">Line Chart</Property>
                </MenuItem>
                <MenuItem>
                  <Property name="caption">Area Chart</Property>
                </MenuItem>
                <MenuItem>
                  <Property name="caption">Radar Chart</Property>
                </MenuItem>
                <MenuItem>
                  <Property name="caption">Pie Chart</Property>
                </MenuItem>
              </MenuItem>
              <MenuItem>
                <Property name="caption">Widget</Property>
                <MenuItem>
                  <Property name="caption">Clock</Property>
                </MenuItem>
              </MenuItem>
            </Menu>
          </Children>
        </Panel>
      </Children>
    </FloatContainer>
  </View>
</ViewConfig>
