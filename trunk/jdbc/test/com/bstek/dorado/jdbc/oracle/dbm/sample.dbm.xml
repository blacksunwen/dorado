<?xml version="1.0" encoding="UTF-8"?>
<DbModel jdbcEnviroment="ora11">
	<Table name="EMPLOYEE" tableName="EMPLOYEE">
	  <Columns>
	    <KeyColumn propertyName="ID" jdbcType="INTEGER-Integer" columnName="ID"/>
	    <Column propertyName="REPORT_TO" jdbcType="INTEGER-Integer" columnName="REPORT_TO"/>
	    
	    <Column propertyName="HIRE_DATE" jdbcType="DATE-Date" columnName="HIRE_DATE"/>
        <Column propertyName="BIRTHDAY" jdbcType="DATE-Date" columnName="BIRTHDAY"/>
        
        <Column propertyName="SEX" jdbcType="CHAR-Boolean" columnName="SEX"/>
        
        <Column propertyName="NOTES" jdbcType="CLOB-String" columnName="NOTES"/>
        
	    <Column propertyName="FIRST_NAME" jdbcType="VARCHAR-String" columnName="FIRST_NAME"/>
	    <Column propertyName="LAST_NAME" jdbcType="VARCHAR-String" columnName="LAST_NAME"/>
	    <Column propertyName="DEPT_ID" jdbcType="VARCHAR-String" columnName="DEPT_ID"/>
	    <Column propertyName="COUNTRY" jdbcType="VARCHAR-String" columnName="COUNTRY"/>
        <Column propertyName="ADDRESS" jdbcType="VARCHAR-String" columnName="ADDRESS"/>
        <Column propertyName="PHONE" jdbcType="VARCHAR-String" columnName="PHONE"/>
        <Column propertyName="PHOTO_PATH" jdbcType="VARCHAR-String" columnName="PHOTO_PATH"/>
	    <Column propertyName="CITY" jdbcType="VARCHAR-String" columnName="CITY"/>
	    <Column propertyName="POSTAL_CODE" jdbcType="VARCHAR-String" columnName="POSTAL_CODE"/>
	    <Column propertyName="TITLE_OF_COURTESY" jdbcType="VARCHAR-String" columnName="TITLE_OF_COURTESY"/>
	    <Column propertyName="REGION" jdbcType="VARCHAR-String" columnName="REGION"/>
	    <Column propertyName="TITLE" jdbcType="VARCHAR-String" columnName="TITLE"/>
	  </Columns>
	  <Property name="dynamicClause"><![CDATA[#if ($ID) WHERE ID=:ID #end]]></Property>
	</Table>

	<Table name="DEPT" tableName="DEPT">
	  <Columns>
	    <KeyColumn propertyName="DEPT_ID" jdbcType="VARCHAR-String" columnName="DEPT_ID"/>
	    <Column propertyName="DEPT_NAME" jdbcType="VARCHAR-String" columnName="DEPT_NAME"/>
	    <Column propertyName="PARENT_ID" jdbcType="VARCHAR-String" columnName="PARENT_ID"/>
	  </Columns>
	  <Property name="dynamicClause"><![CDATA[#if ($ID) WHERE DEPT_ID=:ID #end]]></Property>
	</Table>
</DbModel>