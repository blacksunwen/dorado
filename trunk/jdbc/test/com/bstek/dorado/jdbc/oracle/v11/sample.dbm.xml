<?xml version="1.0" encoding="UTF-8"?>
<DbModel jdbcEnviroment="ora11">
    <Table name="EMPLOYEE" tableName="EMPLOYEE">
      <Columns>
        <KeyColumn jdbcType="INTEGER-Integer" name="ID" />
        <Column jdbcType="INTEGER-Integer" name="REPORT_TO" />
        
        <Column jdbcType="DATE-Date" name="HIRE_DATE" />
        <Column jdbcType="DATE-Date" name="BIRTHDAY"  />
        
        <Column jdbcType="CHAR-Boolean" name="SEX"/>
        
        <Column jdbcType="CLOB-String" name="NOTES"/>
        
        <Column jdbcType="VARCHAR-String" name="FIRST_NAME"/>
        <Column jdbcType="VARCHAR-String" name="LAST_NAME"/>
        <Column jdbcType="VARCHAR-String" name="DEPT_ID"/>
        <Column jdbcType="VARCHAR-String" name="COUNTRY"/>
        <Column jdbcType="VARCHAR-String" name="ADDRESS"/>
        <Column jdbcType="VARCHAR-String" name="PHONE"/>
        <Column jdbcType="VARCHAR-String" name="PHOTO_PATH"/>
        <Column jdbcType="VARCHAR-String" name="CITY"/>
        <Column jdbcType="VARCHAR-String" name="POSTAL_CODE"/>
        <Column jdbcType="VARCHAR-String" name="TITLE_OF_COURTESY"/>
        <Column jdbcType="VARCHAR-String" name="REGION"/>
        <Column jdbcType="VARCHAR-String" name="TITLE"/>
      </Columns>
      <Property name="dynamicClause"><![CDATA[#if ($ID) WHERE ID=:ID #end]]></Property>
    </Table>

    <Table name="DEPT" tableName="DEPT" trigger="com.bstek.dorado.jdbc.oracle.v11.DeptTableTrigger">
      <Columns>
        <KeyColumn jdbcType="VARCHAR-String" name="DEPT_ID"/>
        <Column jdbcType="VARCHAR-String" name="DEPT_NAME"/>
        <Column jdbcType="VARCHAR-String" name="PARENT_ID"/>
      </Columns>
      <Property name="dynamicClause"><![CDATA[#if ($ID) WHERE DEPT_ID=:ID #end]]></Property>
    </Table>

	<Table name="CATEGORIES" tableName="CATEGORIES">
		<Columns>
			<KeyColumn name="ID" jdbcType="INTEGER-Integer"/>
			<Column name="CATEGORY_NAME" jdbcType="VARCHAR-String"/>
			<Column name="DESCRIPTION" jdbcType="VARCHAR-String"/>
		</Columns>
	</Table>
	
	<Table name="PRODUCTS" tableName="PRODUCTS">
		<Columns>
			<KeyColumn name="ID" jdbcType="INTEGER-Integer"/>
			<Column name="PRODUCT_NAME" jdbcType="VARCHAR-String"/>
			<Column name="SUPPLIER_ID"  jdbcType="INTEGER-Integer"/>
			<Column name="CATEGORY_ID"  jdbcType="INTEGER-Integer"/>
			<Column name="QUANTITY_PER_UNIT" jdbcType="VARCHAR-String"/>
			<Column name="UNIT_PRICE"        jdbcType="DECIMAL-BigDecimal"/>
			<Column name="UNITS_IN_STOCK" jdbcType="INTEGER-Integer"/>
			<Column name="UNITS_ON_ORDER" jdbcType="INTEGER-Integer"/>
			<Column name="REORDER_LEVEL"  jdbcType="INTEGER-Integer"/>
			<Column name="DISCONTINUED"   jdbcType="SMALLINT-Short"/>
		</Columns>
	</Table>
	
    <SqlTable name="sql_dept" mainTable="DEPT">
	  <Columns>
	    <Column name="DEPT_NAME"  nativeColumn="DEPT_NAME"/>
	    <Column name="DEPT_ID"    nativeColumn="DEPT_ID"/>
	    <Column name="PARENT_ID"  nativeColumn="PARENT_ID"/>
	  </Columns>
	  <Property name="querySql"><![CDATA[select * from DEPT]]></Property>
	</SqlTable>
	
