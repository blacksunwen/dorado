<?xml version="1.0" encoding="UTF-8"?>
<DbModel>
  <Table name="T1" tableName="T1">
    <Property name="dynamicClause"><![CDATA[
where 1=1

#if ($id) 
 and ID=:id
#end

#if ($c1)
 and c1 like :%c1%
#end
]]></Property>
    <Columns>
      <KeyColumn name="ID">
        <Property name="jdbcType">INTEGER-Integer</Property>
      </KeyColumn>
      <Column name="C1">
        <Property name="jdbcType">VARCHAR-String</Property>
      </Column>
    </Columns>
  </Table>
</DbModel>
