<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="dorado.jdbc.abstractKeyGenerator" abstract="true">
		<property name="manager" ref="dorado.jdbc.keyGeneratorManager"/>
	</bean>

	<bean class="com.bstek.dorado.jdbc.model.table.IdentityKeyGenerator" parent="dorado.jdbc.abstractKeyGenerator"/>
	<bean class="com.bstek.dorado.jdbc.model.table.JdkUuidKeyGenerator" parent="dorado.jdbc.abstractKeyGenerator"/>
	<bean class="com.bstek.dorado.jdbc.model.table.SequenceKeyGenerator" parent="dorado.jdbc.abstractKeyGenerator"/>
	
</beans>
