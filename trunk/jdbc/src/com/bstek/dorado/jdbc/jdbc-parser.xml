<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:d="http://www.bstek.com/dorado/schema"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.bstek.com/dorado/schema http://www.bstek.com/dorado/schema/spring-dorado-7.0.xsd">

	<bean id="dorado.jdbc.jdbcEnviromentParser" class="com.bstek.dorado.jdbc.config.xml.JdbcEnviromentParser" parent="dorado.propertyParser"/>
	
	<bean id="dorado.jdbc.transactionTemplateParser" class="com.bstek.dorado.jdbc.config.xml.TransactionTemplateParser" parent="dorado.propertyParser"/>
	
	<bean id="dorado.jdbc.triggerParser" class="com.bstek.dorado.jdbc.config.xml.TriggerParser" parent="dorado.propertyParser"/>
	
	<bean id="dorado.jdbc.jdbcTypeParser" class="com.bstek.dorado.jdbc.config.xml.JdbcTypeParser" parent="dorado.propertyParser"/>
	
	<bean id="dorado.jdbc.keyGeneratorParser" class="com.bstek.dorado.jdbc.model.table.KeyGeneratorParser" parent="dorado.propertyParser"/>
	
	<bean id="dorado.jdbc.tableReferenceParser" class="com.bstek.dorado.jdbc.model.table.TableRefrenceParser" parent="dorado.propertyParser" />

	<bean id="dorado.jdbc.dbmParser" class="com.bstek.dorado.jdbc.config.xml.DbModelParser" parent="dorado.objectParser"/>
	
	<bean id="dorado.jdbc.tableParser" class="com.bstek.dorado.jdbc.model.table.TableParser" parent="dorado.objectParser">
		<property name="dataTypeManager" ref="dorado.dataTypeManager"/>
	</bean>	
</beans>
