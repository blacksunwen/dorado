<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:d="http://www.bstek.com/dorado/schema"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.bstek.com/dorado/schema http://www.bstek.com/dorado/schema/spring-dorado-7.0.xsd">

	<bean id="dorado.jdbc.jdbcEnviromentParser" class="com.bstek.dorado.jdbc.config.JdbcEnviromentParser" parent="dorado.propertyParser"/>
	
	<bean id="dorado.jdbc.platformTransactionManagerBeanPropertyParser" class="com.bstek.dorado.jdbc.config.PlatformTransactionManagerBeanPropertyParser" parent="dorado.propertyParser"/>
	<bean id="dorado.jdbc.transactionDefinitionBeanPropertyParser" class="com.bstek.dorado.jdbc.config.TransactionDefinitionBeanPropertyParser" parent="dorado.propertyParser"/>
	
	<bean id="dorado.jdbc.triggerParser" class="com.bstek.dorado.jdbc.config.TriggerParser" parent="dorado.propertyParser"/>
	<bean id="dorado.jdbc.jdbcTypeParser" class="com.bstek.dorado.jdbc.config.JdbcTypeParser" parent="dorado.propertyParser"/>
	<bean id="dorado.jdbc.keyGeneratorParser" class="com.bstek.dorado.jdbc.model.table.KeyGeneratorParser" parent="dorado.propertyParser"/>
	<bean id="dorado.jdbc.tableReferenceParser" class="com.bstek.dorado.jdbc.model.table.TableReferenceParser" parent="dorado.propertyParser" />

	<bean id="dorado.jdbc.dbmParser" class="com.bstek.dorado.jdbc.config.DbModelParser" parent="dorado.objectParser"/>
	
	<bean id="dorado.jdbc.abstractTableParser" parent="dorado.objectParser" abstract="true"/>
	
	<bean id="dorado.jdbc.tableParser" class="com.bstek.dorado.jdbc.model.table.TableParser" parent="dorado.jdbc.abstractTableParser"/>
	<bean id="dorado.jdbc.sqlTableParser" class="com.bstek.dorado.jdbc.model.sqltable.SqlTableParser" parent="dorado.jdbc.abstractTableParser"/>
	<bean id="dorado.jdbc.autoTableParser" class="com.bstek.dorado.jdbc.model.autotable.AutoTableParser" parent="dorado.jdbc.abstractTableParser"/>
</beans>
